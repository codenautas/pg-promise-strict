{
  "name": "pg-promise-strict",
  "description": "postgresql with strict interpretation of promises",
  "version": "0.4.2",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/pg-promise-strict",
  "files": [],
  "contributors": [{
    "name": "Emilio Platzer", "email": "emilioplatzer@gmail.com"
  }],
  "dependencies": {
    "fs-extra": "~1.1.1",
    "pg": "~7.3.0",
    
    "best-globals": "~0.10.1",
    "like-ar": "~0.2.2"
  },
  "devDependencies": {
    "colors": "~1.1.2",
    "expect.js": "~0.3.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.5.2",
    
    "discrepances": "~0.2.2",
    "mini-tools": "~0.3.17",
    "type-store": "~0.2.4"
  },
  "optionalDependencies": {
    "type-store": "~0.2.4"
  },
  "main": "pg-promise-strict.js",
  "engines": {
    "node": ">= 4.4"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-real": "mocha --reporter spec --bail --check-leaks test/test-with-real-db.js",
    "test-intensive": "mocha --reporter spec --bail --check-leaks test/intensive-tests.js",
    "test-common": "mocha --reporter spec --bail --check-leaks test/common-tests.js",
    "try-t": "mocha --reporter spec --bail --check-leaks test/trying-timeout.js",
    "test-ci":  "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "start": "node examples/use-examples.js"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "extending",
    "coverage": 100
  }
}
