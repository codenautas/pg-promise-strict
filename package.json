{
  "name": "pg-promise-strict",
  "description": "postgresql with promises and strict types and returning size of results",
  "version": "1.0.0",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/pg-promise-strict",
  "main": "dist/lib/pg-promise-strict.js",
  "types": "dist/lib/pg-promise-strict.d.ts",
  "files": [
    "dist/lib"
  ],
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "dependencies": {
    "fs-extra": "~7.0.1",
    "pg": "~7.8.1",
    "pg-copy-streams": "~1.2.0",
    "best-globals": "~0.10.10",
    "like-ar": "~0.2.9"
  },
  "devDependencies": {
    "@types/node": "~11.10.4",
    "@types/fs-extra": "~5.0.5",
    "@types/pg": "~7.4.13",
    "@types/pg-copy-streams": "~1.2.0",
    "colors": "~1.3.3",
    "expect.js": "~0.3.1",
    "nyc": "~13.3.0",
    "mocha": "~6.0.2",
    "source-map-support": "~0.5.10",
    "ts-node": "~8.0.2",
    "discrepances": "~0.2.4",
    "mini-tools": "~1.0.6",
    "type-store": "~0.2.31",
    "line-splitter": "~0.5.0"
  },
  "optionalDependencies": {
    "type-store": "~0.2.31"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-real": "mocha --reporter spec --bail --check-leaks test/test-with-real-db.js",
    "test-intensive": "mocha --reporter spec --bail --check-leaks test/intensive-tests.js",
    "test-common": "mocha --reporter spec --bail --check-leaks test/common-tests.js",
    "try-t": "mocha --reporter spec --bail --check-leaks test/trying-timeout.js",
    "test-ci": "nyc node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov2": "nyc node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "node node_modules/nyc/bin/nyc node_modules/mocha/bin/_mocha --exit --check-leaks test/",
    "start": "node examples/use-examples.js"
  },
  "nyc": {
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "extension": [
      ".js"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/test",
      "test"
    ],
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true,
    "check-coverage": true
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "extending",
    "coverage": 100
  }
}
