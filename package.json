{
  "name": "pg-promise-strict",
  "description": "postgresql with promises and strict types and returning size of results",
  "version": "1.4.5",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "license": "MIT",
  "repository": "codenautas/pg-promise-strict",
  "main": "dist/lib/pg-promise-strict.js",
  "types": "dist/lib/pg-promise-strict.d.ts",
  "files": [
    "dist",
    "dist/lib"
  ],
  "contributors": [
    {
      "name": "Emilio Platzer",
      "email": "emilioplatzer@gmail.com"
    }
  ],
  "dependencies": {
    "best-globals": "^2.0.7",
    "cast-error": "^0.1.3",
    "fs-extra": "^11.3.3",
    "like-ar": "^0.5.2",
    "pg": "^8.18.0",
    "pg-copy-streams": "^7.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/pg-copy-streams": "^1.2.5",
    "colors": "^1.4.0",
    "discrepances": "^0.2.9",
    "expect.js": "^0.3.1",
    "line-splitter": "^0.8.0",
    "mini-tools": "^1.13.4",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "type-store": "^0.4.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "test": "tsc -p . && mocha --exit test/",
    "test-real": "mocha --reporter spec --bail --check-leaks test/test-with-real-db.js",
    "test-intensive": "mocha --reporter spec --bail --check-leaks test/intensive-tests.js",
    "test-common": "mocha --reporter spec --bail --check-leaks test/common-tests.js",
    "try-t": "mocha --reporter spec --bail --check-leaks test/trying-timeout.js",
    "start": "node examples/use-examples.js"
  },
  "nyc": {
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 99,
    "extension": [
      ".js",
      ".ts"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  },
  "qa-control": {
    "package-version": "0.3.0",
    "run-in": "server",
    "test-appveyor": true,
    "type": "lib",
    "stage": "extending",
    "coverage": 100
  }
}
